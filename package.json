{
  "name": "@shiphub/eslint-config",
  "version": "0.0.3",
  "description": "Shiphub's ESLint config.",
  "keywords": [
    "eslint",
    "eslintconfig",
    "styleguide"
  ],
  "author": {
    "name": "Christopher Szatmary"
  },
  "repository": "getshiphub/eslint-config",
  "bugs": {
    "url": "https://github.com/getshiphub/eslint-config/issues"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "best_practices.js",
    "import.js",
    "index.js",
    "typescript.js"
  ],
  "engines": {
    "node": ">= 20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "fmt": "prettier --write '**/*.js'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "yarn lint",
    "release": "release-it"
  },
  "dependencies": {
    "eslint-config-prettier": "^9.0.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "release-it": "^16.2.1",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "extends": [
      "./index.js"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "printWidth": 100
  }
}
